@model Website.Models.SearchPagingModel
<label>Paging</label>
<div>
    <ul class="pagination">
        <li><a href="#">&laquo;</a></li>
        @for (var i = 0; i < @Model.NumberOfPages; i++)
        {
            <li><a href="#@i">@(i + 1)</a></li>
        }
        <li><a href="#">&raquo;</a></li>
    </ul>
</div>
